<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ExamenSysone</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>candidate</servlet-name>
		<servlet-class>com.sysone.CandidateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>candidate</servlet-name>
		<url-pattern>/candidate</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>compress</servlet-name>
		<servlet-class>com.sysone.CompressServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>compress</servlet-name>
		<url-pattern>/compress</url-pattern>
	</servlet-mapping>

</web-app>